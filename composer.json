{
    "name": "mazedlx/socialsecurity",
    "description": "PHP package to validate Austrian social security numbers",
    "keywords": [
        "social security",
        "validation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Leo-Pernold",
            "email": "mazedlx@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "tightenco/collect": "^8.34"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mazedlx\\SocialSecurity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazedlx\\SocialSecurity\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
